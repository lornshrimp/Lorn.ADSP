{
    "TargetingMatchers": {
        "EnableFSharpMatchers": true,
        "DefaultLifetime": "Scoped",
        "DefaultTimeoutMs": 5000,
        "EnableParallelExecution": true,
        "MaxConcurrentMatchers": 10,
        "Matchers": {
            "demographic": {
                "Enabled": true,
                "Priority": 100,
                "TimeoutMs": 3000,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 50,
                "Configuration": {
                    "AgeRanges": [
                        "18-24",
                        "25-34",
                        "35-44",
                        "45-54",
                        "55+"
                    ],
                    "GenderOptions": [
                        "M",
                        "F",
                        "O"
                    ],
                    "DefaultMatchThreshold": 0.7
                }
            },
            "geolocation": {
                "Enabled": true,
                "Priority": 90,
                "TimeoutMs": 2000,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 30,
                "Configuration": {
                    "DefaultRadius": 50,
                    "Units": "km",
                    "SupportedCountries": [
                        "US",
                        "CA",
                        "GB",
                        "DE",
                        "FR",
                        "CN",
                        "JP"
                    ],
                    "DefaultMatchThreshold": 0.8
                }
            },
            "time": {
                "Enabled": true,
                "Priority": 110,
                "TimeoutMs": 1000,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 10,
                "Configuration": {
                    "TimezoneSupport": true,
                    "DaypartTargeting": true,
                    "SeasonalTargeting": false,
                    "DefaultMatchThreshold": 0.9
                }
            },
            "device": {
                "Enabled": true,
                "Priority": 105,
                "TimeoutMs": 1500,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 20,
                "Configuration": {
                    "SupportedDeviceTypes": [
                        "mobile",
                        "tablet",
                        "desktop",
                        "tv",
                        "wearable"
                    ],
                    "SupportedOperatingSystems": [
                        "iOS",
                        "Android",
                        "Windows",
                        "macOS",
                        "Linux"
                    ],
                    "BrowserTargeting": true,
                    "DefaultMatchThreshold": 0.75
                }
            },
            "behavioral": {
                "Enabled": true,
                "Priority": 95,
                "TimeoutMs": 4000,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 80,
                "Configuration": {
                    "InterestCategories": [
                        "technology",
                        "sports",
                        "entertainment",
                        "travel",
                        "finance"
                    ],
                    "LookbackDays": 30,
                    "MinInteractionThreshold": 5,
                    "DefaultMatchThreshold": 0.6
                }
            },
            "contextual": {
                "Enabled": true,
                "Priority": 85,
                "TimeoutMs": 2500,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 40,
                "Configuration": {
                    "ContentCategories": [
                        "news",
                        "entertainment",
                        "sports",
                        "technology",
                        "business"
                    ],
                    "KeywordTargeting": true,
                    "SentimentAnalysis": false,
                    "DefaultMatchThreshold": 0.65
                }
            },
            "frequencyCap": {
                "Enabled": true,
                "Priority": 120,
                "TimeoutMs": 1000,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 15,
                "Configuration": {
                    "DefaultCapPeriod": "24h",
                    "SupportedPeriods": [
                        "1h",
                        "6h",
                        "12h",
                        "24h",
                        "7d",
                        "30d"
                    ],
                    "MaxImpressions": 10,
                    "DefaultMatchThreshold": 1.0
                }
            },
            "budget": {
                "Enabled": true,
                "Priority": 130,
                "TimeoutMs": 1500,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 25,
                "Configuration": {
                    "CurrencySupport": [
                        "USD",
                        "EUR",
                        "GBP",
                        "JPY",
                        "CNY"
                    ],
                    "BudgetTypes": [
                        "daily",
                        "weekly",
                        "monthly",
                        "total"
                    ],
                    "AllowOverspend": false,
                    "DefaultMatchThreshold": 0.95
                }
            },
            "audience": {
                "Enabled": true,
                "Priority": 115,
                "TimeoutMs": 3500,
                "CanRunInParallel": true,
                "ExpectedExecutionTimeMs": 60,
                "Configuration": {
                    "AudienceTypes": [
                        "lookalike",
                        "custom",
                        "interest",
                        "demographic"
                    ],
                    "SimilarityThreshold": 0.8,
                    "AudienceSize": {
                        "Min": 1000,
                        "Max": 10000000
                    },
                    "DefaultMatchThreshold": 0.7
                }
            }
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Lorn.ADSP.Infrastructure.DependencyInjection": "Debug",
            "Lorn.ADSP.Strategies.Targeting": "Debug"
        }
    },
    "HealthChecks": {
        "EnableDetailedErrors": true,
        "Timeout": "00:00:30"
    }
}